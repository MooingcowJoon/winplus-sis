<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.samyang.winplus.common.system.error.dao.ErrorDao">
		
	<insert id="insertSystemErrorLog" parameterType="java.util.Map">
		/* insertSystemErrorLog */
		INSERT INTO COM_SYSTEM_ERROR_LOG( 
			EMP_NO
			, EMP_NO_ORIZ
			, CONN_MENU_CD
			, CONN_IP
			, CONN_URI
			, CRUD
			, REQUEST_WITH
			, ERROR_NOTE
			, REG_DT
			, REG_ID
		) values (
			#{EMP_NO}
			, #{EMP_NO_ORIZ}
			, #{CONN_MENU_CD}
			, #{CONN_IP}
			, #{CONN_URI}
			, #{CRUD}
			, #{REQUEST_WITH}
			, #{ERROR_NOTE}
			, GETDATE()
			, #{EMP_NO}
		)
	</insert>
	
</mapper>
